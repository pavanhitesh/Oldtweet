{
  "name": "@tweet/tweet",
  "version": "1.0.0",
  "description": "An E2E Test Automation Framework for Web, Mobile and API's",
  "private": true,
  "main": "index.js",
  "scripts": {
    "clean": "npx lerna run clean --parallel && npx shx rm -rf node_modules",
    "setup": "npm install --legacy-peer-deps && npm run bootstrap",
    "bootstrap": "npx lerna bootstrap --hoist",
    "tsc": "npx cross-env npx tsc -p tsconfig.json",
    "lint": "npm run tsc && npx cross-env npx eslint . --ext .json,.eslintrc,.eslintignore,.js,.ts --fix && npx cross-env npx gherkin-lint -c .lint.feature && npm run prettier",
    "prettier": "cross-env npx prettier --config .prettierrc --write '**/*.ts'",
    "hello": "npx lerna run hello --parallel",
    "postinstall": "husky install",
    "commit": "npx commit"
  },
